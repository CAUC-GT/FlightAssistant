.plusBtn {
  position: fixed;
  z-index: 100;
  bottom: 100rpx;
  right: 100rpx;
  background-color: #8bc34a;
  border-radius: 50%;
  width: 120rpx;
  height: 120rpx;
  box-shadow: 0px 2px 4px 2px #DDDDDD;
  border-width: 1rpx;
}

.plusIcon {
  font-size: 60rpx;
  position: relative;
  margin: 15px;
  color: #fafafa;
}