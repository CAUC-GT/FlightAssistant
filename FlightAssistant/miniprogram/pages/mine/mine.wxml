<view>
	<view class="card">
		<van-row>
			<van-col span="8">
				<van-image round fit="cover" width="5rem" height="5rem" class="avatar" src="{{avatarUrl}}" />
			</van-col>
			<van-col span="14" offset="2">
				<van-row>
					<text class="name">用户名:{{nickName}}</text>
				</van-row>
			</van-col>
		</van-row>
	</view>
	<view class="enter1">
		<van-cell title="历史待办回收站" is-link link-type="navigateTo" url="../history/history" />
	</view>
	<view class="enter1">
		<van-cell title="帮助" is-link link-type="navigateTo" url="../help/help" />
	</view>

	<view class="enter2">
		<navigator open-type="exit" target="miniProgram">
			<van-cell title="退出" title-style="color:red" />
		</navigator>
	</view>
</view>